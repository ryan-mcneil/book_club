<%= form_for [@book] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :authors %>
  <%= f.checkbox_field :authors, value: "or author(s)" %>
  <%=f.label :pages %>
  <%=f.number_field :pages %>
  <%=f.label :year %>
  <%=f.number_field :year %>
   <%= f.submit %>
<% end %>
