<h1>Create a New Review</h1>
<ul>
  <% @article.errors.full_messages.each do |error| %>
    <li> <%= error %></li>
    <% end %>
</ul>
<p>
  <%= f.label :title %><br />
  <%= f.text_field :title %>
</p>
<p>
  <%= f.label :score %><br />
  <%= f.text_field :score %>
</p>
<p>
  <%= f.label :description %><br />
  <%= f.text_area :description %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>
